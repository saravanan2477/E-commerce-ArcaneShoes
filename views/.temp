    <div class="section__content">
          

            <div class="container">

                <div class="roww">

                    <div class="col-lg-12 col-md-12 col-sm-12">
                        <!--====== Wishlist Product ======-->
                        <div class="w-r u-s-m-b-30">
                            <div class="w-r__container">
                              <% if (isEmpty) { %>




                                <div class="w-r__wrap-1">
                        
                            <div class="roww">
                                <div class="col-md-12 text-center">
                                  <br>
                                  <p style="color: red;">Your wishlist is empty. Start adding products now!</p>
                                </div>
                              </div>
                              <% } else { %>
                                <% Wishlist.forEach(item => { %>

                                    <div class="w-r__img-wrap">

                                        <img class="u-img-fluid" src="<%= item.image %>" alt=""></div>
                                    <div class="w-r__info">

                                        <span class="w-r__name">

                                            <a href="/product/<%= item.productid %>">  <%= item.product %></a></span>

                                        <span class="w-r__category">

                                            <a href="shop-side-version-2.html">category name</a></span>

                                            <label class="w-r__price">â‚¹<%= item.price %></label>

                                            <!-- <span class="w-r__discount">$160.00</span></sp an></div> -->
                                
                                            
                                        </div>

                                       


                                <div class="w-r__wrap-2">

                                    <a class="w-r__link btn--e-brand-b-2" href="/addcart/<%= item.productid %>">TO CART</a>

                                    <a class="w-r__link btn--e-transparent-platinum-b-2" href="/getproduct/<%= item.productid %>">VIEW</a>

                                    <a class="w-r__link btn--e-transparent-platinum-b-2" href="/removeFromWishlist/<%=item._id %>">REMOVE</a></div>
                            </div>
                           
                        <% }); %>
                        <% } %>
                        </div>
                        <!--====== End - Wishlist Product ======-->




                      
                    </div>
                    <div class="col-lg-12">
                        <div class="route-box">
                            <div class="route-box__g">

                                <a class="route-box__link" href="shop-side-version-2.html"><i class="fas fa-long-arrow-alt-left"></i>

                                    <span>CONTINUE SHOPPING</span></a></div>
                            <div class="route-box__g">

                                <a class="route-box__link" href="wishlist.html"><i class="fas fa-trash"></i>

                                    <span>CLEAR WISHLIST</span></a></div>
                        </div>
                    </div>


                    
                </div>
            </div>
        </div>
        <!--====== End - Section Content ======-->
    </div>